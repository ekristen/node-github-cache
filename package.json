{
  "name": "github-cache",
  "author": "Erik Kristensen <erik@erikkristensen.com>",
  "homepage": "https://github.com/ekristen/node-github-cache",
  "description": "Transparent caching layer for node-github module",
  "version": "3.0.0-beta.1",
  "main": "./dist/cache.js",
  "repository": "ekristen/node-github-cache",
  "bugs": "https://github.com/ekristen/node-github-cache/issues",
  "license": "MIT",
  "keywords": [
    "octokit",
    "github",
    "cache",
    "promises"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "npm run lint:ts && npm run lint:type",
    "lint:ts": "tslint -c ./tslint.json -p ./tsconfig.json 'src/**/*.{ts,tsx}'",
    "lint:type": "tsc --noEmit",
    "lint:fix": "tslint -c ./tslint.json -p ./tsconfig.json --fix 'src/**/*.{ts,tsx}'",
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*.spec.ts",
    "validate": "npm ls"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "object-hash": "^1.3.0"
  },
  "devDependencies": {
    "@octokit/rest": "^15.9.4",
    "@types/bunyan": "^1.8.5",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.2",
    "bunyan": "^1.8.12",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "precommit-hook": "^3.0.0",
    "through2": "^2.0.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ]
}
